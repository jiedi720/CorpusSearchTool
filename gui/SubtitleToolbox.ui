    <string notr="true">/* 统一设置所有项的默认状态 */
QMenuBar::item {
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 5px;
    padding-bottom: 5px;
    /* 关键：默认也给1px边框，但颜色设为透明，防止点击时文字抖动 */
    border: 1px solid transparent; 
    border-radius: 5px;
}

/* 菜单栏整体样式 */
QMenuBar {
    border-bottom: 1px solid #dcdcdc; /* 这就是那条浅灰色的线 */
    padding: 0px; /* 增加一点内边距，让菜单项不紧贴边框 */
}

/* 浅色主题下的按下效果 */
QMenuBar[theme=&quot;light&quot;]::item:pressed {
    background: #e0e0e0; 
    border: 1px solid transparent;
    border-radius: 5px;
}

/* 深色主题下的按下效果 */
QMenuBar[theme=&quot;dark&quot;]::item:pressed {
	color: rgb(255, 255, 255);
    background: #989898; 
    border: 1px solid transparent;
    border-radius: 5px;
}

/* 下拉菜单样式 */
QMenu {
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 2px;
    border: 1px solid #e0e0e0;
    border-radius: 9px;  
}

QMenu::item:selected {
    background-color: #FFC209;
    color: black; /* 显式设置文字颜色，防止被原生反色覆盖 */
    border: 1px solid #FFC209; /* 关键：加上边框，哪怕颜色和背景一样 */
    border-radius: 4px; /* 让选中的高亮块也有圆角，看起来更现代 */
}</string>
