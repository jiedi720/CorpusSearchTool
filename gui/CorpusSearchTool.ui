            <property name="styleSheet">
             <string notr="true">/* 整体容器美化 */
QTabWidget[theme=&quot;light&quot;]::pane {
    background-color: #DEDEDE;   
    border-radius: 10px;            
    border-top-left-radius: 0px;   
    border-top-right-radius: 0px;  
    top: 0px;                     
}

QTabWidget[theme=&quot;dark&quot;]::pane {
    background-color: #9e9e9e;    
    border-radius: 10px;            
    border-top-left-radius: 0px;   
    border-top-right-radius: 0px;  
    top: 0px;                     
}

/* 所有标签（默认色）：黄色 */
/* 注意：这里作为默认值，会被 first 和 last 覆盖 */
QTabBar::tab { 
    color: rgb(0, 0, 0);
    background-color: #FFC209;
    padding: 2px 1px;/*设置标签文字与标签边框之间的留白区域 */
    min-width: 150px; /*设置标签的最小宽度 */
    margin-right: 1px; /* 默认保留边距用于重叠 */
    /* 核心修改：分别设置四个角的弧度 顺序为：左上, 右上, 右下, 左下 */
    border-top-left-radius: 10px;  
    border-top-right-radius: 10px; 
    border-bottom-left-radius: 1px;
    border-bottom-right-radius: 1px;
}

/* 选中状态 */
QTabBar::tab:selected {
    font-weight: bold;
    color: rgb(255, 255, 255);
    background-color: #7953B1;
    /* 3. 这里的 margin-top 设为 0，配合基础态的 2px，实现相对升起效果 */
    margin-top: 0px; 
    /* 再次明确圆角，确保不被默认样式覆盖 */
    border-top-left-radius: 9px;
    border-top-right-radius: 9px;
}</string>
