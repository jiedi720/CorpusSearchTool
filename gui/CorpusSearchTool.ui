QPushButton:hover {
    background-color: rgb(0, 214, 196); /* 颜色比默认稍亮 */
}

QPushButton:pressed {
    /* 1. 向下推文字 */
    padding-top: 5px; 
    /* 2. 同时减少底部，确保内部有效空间高度不变 */
    padding-bottom: 0px; 
    /* 3. 强制内容水平垂直居中，防止对齐方式干扰 */
    text-align: center;
}</string>
